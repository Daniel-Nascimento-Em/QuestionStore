@model CadastroParticipanteModel
@{
    ViewData["Title"] = "Cadastro de Participante";
}
<h3>
    @ViewData["Title"]
</h3>
<hr />

<div class="container">

    <vc:summary />

    <form asp-action="FormularioContato" asp-anti-forgery="true" method="post">

        <div class="form-group">
            <label asp-for="Nome">Nome</label>
            <input asp-for="Nome" class="form-control" placeholder="Informe o nome" />
        </div>
        <div class="form-group">
            <label asp-for="Idade">Idade</label>
            <input asp-for="Idade" class="form-control" placeholder="Informe a idade" />
        </div>
        <input type="submit" value="Salvar Informação" class="btn btn-primary" />
    </form>

</div>


