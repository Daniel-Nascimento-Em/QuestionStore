@model CadastroParticipanteModel
@{
    ViewData["Title"] = "Cadastro de Participante";
}
<h3>
    @ViewData["Title"]
</h3>
<hr />

<div class="container">

    <vc:summary />

    <form asp-action="FormularioContato" asp-anti-forgery="true" method="post">
        <div asp-validation-summary="ModelOnly" class="text-danger"></div>
        <div class="form-group">
            <label asp-for="Nome">Nome</label>
            <input asp-for="Nome" class="form-control" placeholder="Informe o nome" />
        </div>

        <div class="row">
            <div class="col">
                <label asp-for="Idade">Idade</label>
                <input asp-for="Idade" class="form-control" placeholder="Informe a idade" />
            </div>

            <div class="col">
                <label asp-for="CPF">CPF</label>
                <input asp-for="CPF" class="form-control" placeholder="Informe o Cpf" />
            </div>
        </div>
        <div class="row">
            <div class="col">
                <label asp-for="Rua">Rua</label>
                <input asp-for="Rua" class="form-control" placeholder="Informe a rua" />
            </div>

            <div class="col">
                <label asp-for="Complemento">Complemento</label>
                <input asp-for="Complemento" class="form-control" placeholder="Informe a numero" />
            </div>

            <div class="col-1">
                <label asp-for="Numero">Numero</label>
                <input asp-for="Numero" class="form-control" />
            </div>
        </div>

        <div class="row">
            <div class="col">
                <label asp-for="Cidade">Cidade</label>
                <input asp-for="Cidade" class="form-control" placeholder="Informe o numero" />
            </div>
            <div class="col">
                <label asp-for="Estado">Estado</label>
                <input asp-for="Estado" class="form-control" placeholder="Informe o Estado" />
            </div>
            <div class="col">
                <label asp-for="Cep">CEP</label>
                <input asp-for="Cep" class="form-control" placeholder="Informe o Cep" />
            </div>
        </div>

        <div>
            <br />
            <input type="submit" value="Confirmar" class="btn btn-primary" />
        </div>
    </form>

</div>


